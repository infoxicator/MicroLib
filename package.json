{
  "name": "micro-lib",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "installConfig": {
    "pnp": false
  },
  "repository": {
    "url": "https://github.com/module-federation/microlib"
  },
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "build": "npm run clean && webpack --config webpack.config.js && npm run transpile",
    "start": "node -r dotenv/config dist/index.js",
    "dev-start": "nodemon --watch src --exec npm restart",
    "restart": "npm run build && npm run start",
    "transpile": "babel src/index.js -d dist",
    "test": "mocha --recursive -r esm",
    "demo": "open http://localhost:8070",
    "clean": "rm -rf dist"
  },
  "author": "Tyson Midboe",
  "license": "MIT",
  "dependencies": {
    "clear-module": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "import-fresh": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "passport-jwt": "^4.0.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "esm": "^3.2.25",
    "express-cli": "0.0.1",
    "mocha": "^8.2.0",
    "nodemon": "^2.0.6",
    "webpack": "^5.4.0",
    "webpack-cli": "^3.3.12"
  }
}
